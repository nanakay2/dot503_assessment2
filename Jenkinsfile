pipeline {
    agent any
    stages {
        stage('wait for 2mins') {
            steps {
                bat 'timeout /t 10 /nobreak'
                bat 'echo "Hello World"'
            }
        }
        stage('install') {
            steps {
                bat 'set'
            }
        }
        stage('test') {
            steps {
                bat 'set'
            }
        }
        stage('build') {
            steps {
                bat 'set'
            }
        }
    }
}
